<text:p text:style-name="Figure_20_Anchor">
    <draw:frame
        draw:style-name="Figure_20_Frame"
        draw:name="Frame$figure"
        text:anchor-type="char"
        svg:width="$pagewidth"
        style:rel-width="100%"
        draw:z-index="4">
        <draw:text-box fo:min-height="0.499cm">
            <text:p text:style-name="Figure_20_heading">
                <draw:frame draw:style-name="Figure"
                    draw:name="$image"
                    text:anchor-type="paragraph"
                    svg:x="$imgindent"
                    svg:y="0cm"
                    svg:width="$imgwidth"
                    svg:height="$imgheight"
                    draw:z-index="5">
                    <draw:image xlink:href="Pictures/$image.svg"
                        xlink:type="simple"
                        xlink:show="embed"
                        xlink:actuate="onLoad"/>
                </draw:frame>Figure 1-1 Something Weird Happened
            </text:p>
        </draw:text-box>
    </draw:frame>
</text:p>
